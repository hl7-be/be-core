dzop-> dzop : "Validate team"

dzop-> planner : "Return validated team \n[[#teampiet1 CareTeam1]]"

end

    
...


    
group Update team
dragon-> planner : "Add new Team member"

planner-> dzop : "Propose updated team \n[[#teampiet1 CareTeam1]]"

dzop-> dzop : "Validate team"

dzop-> planner : "Return validated team"

planner-> dragon : "Show team composition"

end

  
@enduml

